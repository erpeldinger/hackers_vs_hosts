  	  	5 -> 9 [ label = "0/1/1" ];
  	  	3 -> 5 [ label = "0/1/1" ];
  	  	1 -> 6 [ label = "0/1/1" ];
  	  	1 -> 5 [ label = "0/1/1" ];
  	  	0 -> 4 [ label = "0/1/1" ];
  	  	0 -> 3 [ label = "0/1/1" ];
  	  	0 -> 2 [ label = "0/1/1" ];
